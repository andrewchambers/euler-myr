use std
use prime

/*
	The prime factors of 13195 are 5, 7, 13 and 29.

	What is the largest prime factor of the number 600851475143 ?
*/

const main = {
	var n : uint64 = 775146
	var primes
	
	primes = prime.upto(n)
	while true
		n -= 1
		if 600851475143 % n != 0
			continue
		;;
		match std.bsearch(primes, n, std.numcmp)
		| `std.Some _:
			std.put("{}\n", n)
			break
		| _:
		;;
	;;
}
