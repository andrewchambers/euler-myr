use std
use prime

/*
	The prime factors of 13195 are 5, 7, 13 and 29.

	What is the largest prime factor of the number 600851475143 ?
*/

const main = {
	var n : uint64 = 775146
	while true
		n -= 1
		if !prime.isprime(n)
			continue
		;;
		if 600851475143 % n == 0
			std.put("{}\n", n)
			-> void
		;;
	;;
}
