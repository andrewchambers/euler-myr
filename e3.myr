use std
use prime

/*
	The prime factors of 13195 are 5, 7, 13 and 29.

	What is the largest prime factor of the number 600851475143 ?
*/

const smallestfactor = {n
	for var i = 2; i < n; i++
		if n % i == 0
			-> i
		;;
	;;
	-> n
}

const main = {
	var n : uint64
	var f : uint64
	
	n = 600851475143
	while true
		f = smallestfactor(n)
		if f == n
			break
		;;
		n /= f
	;;
	std.put("{}\n", n)
}
