use std
use prime

/*
	By listing the first six prime numbers: 2, 3, 5, 7, 11, and 13, we can see
	that the 6th prime is 13.
	
	What is the 10 001st prime number?
*/

const main = {
	var n
	var i
	
	n = 0
	i = 1
	while true
		i++
		if prime.isprime(i)
			n += 1
			if n == 10001
				std.put("{}\n", i)
				-> void
			;;
		;;
	;;
}
